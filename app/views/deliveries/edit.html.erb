<h4>配送先編集</h4>
<%= form_for(@delivery) do |f| %>
	<div class="field">
		<%= f.label :郵便番号（ハイフンなし） %>
		<%= f.text_field :postcode %>
	</div>
	<div class="field">
		<%= f.label :住所 %>
		<%= f.text_field :address %>
	</div>
	<div class="field">
		<%= f.label :宛名 %>
		<%= f.text_field :name %>
	</div>
	<div class="field">
		<%= f.submit '編集する', class: "btn btn-success" %>
	</div>
<% end %>
